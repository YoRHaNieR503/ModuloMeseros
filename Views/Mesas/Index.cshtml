@{
}
<h1>0/8 Ocupadas</h1>
<br />
<form>

    <table class="table table-striped">

        <thead>

            <tr>
                <th>Numero de Mesa</th>
                <th>Estado</th>
            </tr>

        </thead>

        <tbody>

            @foreach (var mesas in (IEnumerable<dynamic>)ViewData["listadoMesa"])
            {
                <tr>
                    <td>Mesa # @mesas.Numero_Mesa</td>
                    <td>@mesas.Estado</td>
                </tr>
            }

        </tbody>

    </table>

</form>