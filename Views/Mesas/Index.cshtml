@{
}
<h1>0/8 Ocupadas</h1>
<br />
<form>

    <table class="table table-striped table-dark">

        <thead>

            <tr>
                <th>Numero de Mesa</th>
                <th>Estado</th>
                <th>Reservar</th>
            </tr>

        </thead>

        <tbody>

            @foreach (var mesas in (IEnumerable<dynamic>)ViewData["listadoMesa"])
            {
                <tr>
                    <td>Mesa # @mesas.Numero_Mesa</td>
                    <td>@mesas.Estado</td>
                    <td><button class="btn btn-primary btn-secondary" asp-area="" asp-controller="Home" asp-action="Index">Reservar</button></td>
                </tr>
            }

        </tbody>

    </table>

</form>